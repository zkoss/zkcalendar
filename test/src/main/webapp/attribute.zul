    <div viewModel="@id('vm')@init('test.CalendarAttributeViewModel')" height="100%">
        <toolbar height="50px">
            <toolbarbutton id="readOnlyButton" label="read only" mode="toggle" checked="@bind(vm.readOnly)" onItemCreate=""/><!--ZKCAL-126-->
